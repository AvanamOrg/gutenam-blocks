<wpml-config>
	<gutenberg-blocks>
		<gutenberg-block type="base/advancedbtn" translate="0"/>
		<gutenberg-block type="base/singlebtn" translate="1">
			<key name="link" />
			<key name="text" />
		</gutenberg-block>
		<gutenberg-block type="base/icon" translate="0"/>
		<gutenberg-block type="base/single-icon" translate="1">
			<key name="link" />
			<key name="title" />
		</gutenberg-block>
		<gutenberg-block type="base/advancedheading" translate="1">
			<xpath>//div//*[self::h1 or self::h2 or self::h3 or self::h4 or self::h5 or self::h6 or self::p or self::div]</xpath>
		</gutenberg-block>
		<gutenberg-block type="base/tabs" translate="1">
			<xpath>//span[@class="bst-title-text"]</xpath>
			<xpath>//span[@class="bst-title-sub-text"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="base/infobox" translate="1">
			<xpath>//div/a/@href</xpath>
			<xpath>//div[@class="bst-infobox-textcontent"]//*[self::h1 or self::h2 or self::h3 or self::h4 or self::h5 or self::h6 or self::p]</xpath>
			<xpath>//span[@class="bst-blocks-info-box-learnmore"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="base/pane" translate="1">
			<xpath>//span[@class="bst-blocks-accordion-title"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="base/iconlist" translate="1">
			<xpath>//li[contains(@class, "bst-svg-icon-list-item-wrap")]//span</xpath>
			<xpath>//li[contains(@class, "bst-svg-icon-list-item-wrap")]//a/@href</xpath>
		</gutenberg-block>
		<gutenberg-block type="base/testimonials" translate="1">
			<xpath>//div[@class="bst-testimonial-title-wrap"]//*[self::h1 or self::h2 or self::h3 or self::h4 or self::h5 or self::h6 or self::p]</xpath>
			<xpath>//div[@class="bst-testimonial-content"]</xpath>
			<xpath>//div[@class="bst-testimonial-name"]</xpath>
			<xpath>//div[@class="bst-testimonial-occupation"]</xpath>
		</gutenberg-block>
		<gutenberg-block type="base/testimonial" translate="1">
			<key name="url"/>
			<key name="title"/>
			<key name="content"/>
			<key name="name"/>
			<key name="occupation"/>
		</gutenberg-block>
		<gutenberg-block type="base/form" translate="1">
			<xpath>//form[contains(@class, "bsb-form")]/@*[name()='data-error-message']</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/label/text()</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/a[contains(@class, "bsb-accept-link")]/text()</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/a[contains(@class, "bsb-accept-link")]/@href</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/input/@*[name()='data-label' or name()='placeholder' or name()='value']</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/textarea/@*[name()='data-label' or name()='placeholder']</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/input/@*[name()='data-required-message']</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/textarea/@*[name()='data-required-message']</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/input/@*[name()='data-validation-message']</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/textarea/@*[name()='data-validation-message']</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/textarea</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/span</xpath>
			<xpath>//div[contains(@class, "base-blocks-form-field")]/button</xpath>
			<key name="messages">
				<key name="*">
					<key name="success"/>
					<key name="error"/>
					<key name="required"/>
					<key name="invalid"/>
					<key name="recaptchaerror"/>
				</key>
			</key>
			<key name="redirect"/>
			<key name="email">
				<key name="*">
					<key name="subject"/>
					<key name="fromName"/>
				</key>
			</key>
		</gutenberg-block>
		<gutenberg-block type="base/spacer" translate="0"/>
		<gutenberg-block type="base/rowlayout" translate="0"/>
		<gutenberg-block type="base/column" translate="0"/>
		<gutenberg-block type="base/tab" translate="0"/>
		<gutenberg-block type="base/accordion" translate="0"/>
	</gutenberg-blocks>
</wpml-config>